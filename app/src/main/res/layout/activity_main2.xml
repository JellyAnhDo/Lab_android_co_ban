<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="32dp"
    tools:context=".LoginActivity">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:layout_marginTop="1dp"
        android:gravity="center"
        android:text="Menu"
        android:textSize="32sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingPrefix" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="600dp"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-30dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <Button
                android:id="@+id/listDanhBa"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="44dp"
                android:paddingLeft="32dp"
                android:paddingRight="32dp"
                android:text="Danh Bạ"
                android:textSize="24sp"
                app:layout_constraintTop_toBottomOf="@+id/textView2"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="71dp" />

            <Button
                android:id="@+id/map"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="40dp"
                android:paddingLeft="32dp"
                android:paddingRight="32dp"
                android:text="Map"
                android:textSize="24sp"
                app:layout_constraintTop_toBottomOf="@+id/listDanhBa"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="71dp" />

            <Button
                android:id="@+id/baiHat"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="40dp"
                android:paddingLeft="32dp"
                android:paddingRight="32dp"
                android:text="Nghe nhạc"
                android:textSize="24sp"
                app:layout_constraintTop_toBottomOf="@+id/map"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="68dp" />

            <Button
                android:id="@+id/broadCastBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="40dp"
                android:paddingLeft="32dp"
                android:paddingRight="32dp"
                android:text="Broadcast"
                android:textSize="24sp"
                app:layout_constraintTop_toBottomOf="@+id/baiHat"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="32dp" />

            <Button
                android:id="@+id/profileBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="40dp"
                android:paddingLeft="32dp"
                android:paddingRight="32dp"
                android:text="Profile"
                android:textSize="24sp"
                app:layout_constraintTop_toBottomOf="@+id/broadCastBtn"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="32dp" />

            <Button
                android:id="@+id/listDanhBaV2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="40dp"
                android:paddingLeft="32dp"
                android:paddingRight="32dp"
                android:text="Danh bạ V2"
                android:textSize="24sp"
                app:layout_constraintTop_toBottomOf="@+id/profileBtn"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="32dp" />

            <Button
                android:id="@+id/listDanhBaV3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="40dp"
                android:paddingLeft="32dp"
                android:paddingRight="32dp"
                android:text="Danh bạ V3"
                android:textSize="24sp"
                app:layout_constraintTop_toBottomOf="@+id/listDanhBaV2"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="32dp" />

            <Button
                android:id="@+id/logoutBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="40dp"
                android:paddingLeft="32dp"
                android:paddingRight="32dp"
                android:text="Logout"
                android:textSize="24sp"
                app:layout_constraintTop_toBottomOf="@+id/listDanhBaV3"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="32dp" />
        </LinearLayout>
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>